package com.poultry.service;

import java.util.List;

import com.poultry.model.TripDetails;

public interface TripDetailsService {
    List<TripDetails> findAll();
    TripDetails findById(Long id);
    TripDetails save(TripDetails tripDetails);
    void delete(Long id);
    TripDetails findByTripNumber(int tripNumber);
    Integer getLatestTripNumber();
    List<TripDetails> findByMonthAndYear(int month, int year);
}